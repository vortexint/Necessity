    "terminal.ansiWhite": "#DCDCDC",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBlue": "#5570A0",
    "terminal.ansiBrightBlue": "#6B8CC3",
    "terminal.ansiGreen": "#6A9955",
    "terminal.ansiBrightGreen": "#87B379",
    "terminal.ansiCyan": "#4EC9B0",
    "terminal.ansiBrightCyan": "#69D8C6",
    "terminal.ansiRed": "#D16969",
    "terminal.ansiBrightRed": "#E07C7C",
    "terminal.ansiMagenta": "#B589D6",
    "terminal.ansiBrightMagenta": "#C7A0E0",
    "terminal.ansiYellow": "#C19C00",
    "terminal.ansiBrightYellow": "#D8B51A"
  },
  "tokenColors": [
    {
      "name": "Default Text",
      "scope": [
        "source",
        "text",
        "meta.brace",
        "punctuation",
        "meta.tag",
        "meta.embedded"
      ],
      "settings": {
        "foreground": "#DCDCDC"
      }
    },
    {
      "name": "Variables and Parameters",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.constant",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Parameters (Optional Italic)",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor",
        "keyword.control.directive",
        "punctuation.definition.directive"
      ],
      "settings": { "foreground": "#E07C7C" }
    },
   
    {
      "name": "General Keywords, Modifiers, Namespace/Using",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other.namespace",
       
        "storage",
        "storage.modifier",
        "storage.type.namespace"
      ],
      "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
    },
    {
      "name": "Standard Built-in Types",
      "scope": [
        "storage.type",
       
       
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#959595", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#A0D080" }
    },
    {
      "name": "Numbers and Language Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.character.escape"
      ],
      "settings": { "foreground": "#D68989" }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": { "foreground": "#DCDCAA" }
    },
    {
      "name": "Classes, User-Defined Types, Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.interface",
        "entity.name.enum",
        "support.type",
        "support.class"
      ],
      "settings": { "foreground": "#C94E4E", "fontStyle": "bold" }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#4EC9B0", "fontStyle": "italic" }
    },
    {
      "name": "Tag Brackets",
      "scope": "punctuation.definition.tag",
      "settings": { "foreground": "#808080" }
    },
    {
      "name": "Tag Name",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#4EC9B0", "fontStyle": "italic" }
    },
   
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Punctuation",
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.raw.markdown"
      ],
      "settings": { "foreground": "#808080" }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold", "foreground": "#FFFFFF" }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic", "foreground": "#DCDCDC" }
    },
    {
      "name": "Markdown Link Text",
      "scope": "string.other.link.title.markdown",
      "settings": { "foreground": "#6A9955" }
    },
    {
      "name": "Markdown Link URL",
      "scope": "markup.underline.link.markdown",
      "settings": { "foreground": "#B589D6" }
    },
    {
      "name": "Markdown Raw Code",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": { "foreground": "#D16969" }
    },
    {
      "name": "CSS Tag/Selector Name",
      "scope": "entity.name.tag.css",
      "settings": { "foreground": "#D16969", "fontStyle": "bold" }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id.css",
      "settings": { "foreground": "#4EC9B0" }
    },
    {
      "name": "CSS Class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": { "foreground": "#6A9955" }
    },
    {
      "name": "CSS Property Name",
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#DCDCDC" }
    },
    {
      "name": "CSS Property Value",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.font-name.css"
      ],
      "settings": { "foreground": "#B589D6" }
    },
    {
      "name": "CSS Units",
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#B589D6" }
    }
  ]
}
