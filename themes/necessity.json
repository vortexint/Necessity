  "tokenColors": [
    {
      "name": "Keywords (control & storage)",
      "scope": ["keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#569CD6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Primitives & built-in types",
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "User types (class, struct, enum)",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.enum"
      ],
      "settings": {
        "foreground": "#C94E4E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions & calls",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "name": "Preprocessor & macros",
      "scope": ["meta.preprocessor", "keyword.control.directive"],
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings & char literals",
      "scope": ["string.quoted", "constant.character.escape"],
      "settings": {
        "foreground": "#A0D080"
      }
    },
    {
      "name": "Numeric literals",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#D68989"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Namespaces & using",
      "scope": ["keyword.other.namespace", "entity.name.namespace"],
      "settings": {
        "foreground": "#9CDCFE",
        "fontStyle": "italic"
      }
    }
  ]

}