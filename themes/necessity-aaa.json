  "tokenColors": [
    {
      "name": "Default Text",
      "scope": [
        "source",
        "text",
        "meta.brace",
        "punctuation",
        "meta.tag",
        "meta.embedded"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Variables and Parameters",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.constant",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Parameters (Optional Italic)",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor",
        "keyword.control.directive",
        "punctuation.definition.directive"
      ],
      "settings": { "foreground": "#FF7E7E" }
    },
    {
      "name": "General Keywords, Modifiers, Namespace/Using",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other.namespace",
        "storage",
        "storage.modifier",
        "storage.type.namespace"
      ],
      "settings": { "foreground": "#FF9D00", "fontStyle": "bold" }
    },
    {
      "name": "Standard Built-in Types",
      "scope": [
        "storage.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#4DC4FF"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#9E9E9E", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#B8E785" }
    },
    {
      "name": "Numbers and Language Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.character.escape"
      ],
      "settings": { "foreground": "#FF9E9E" }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": { "foreground": "#FFE066" }
    },
    {
      "name": "Classes, User-Defined Types, Structs",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.interface",
        "entity.name.enum",
        "support.type",
        "support.class"
      ],
      "settings": { "foreground": "#FF7B7B", "fontStyle": "bold" }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#56D4BC", "fontStyle": "italic" }
    },
    {
      "name": "Tag Brackets",
      "scope": "punctuation.definition.tag",
      "settings": { "foreground": "#9E9E9E" }
    },
    {
      "name": "Tag Name",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#FF9D00", "fontStyle": "bold" }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#56D4BC", "fontStyle": "italic" }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FF9D00", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Punctuation",
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.raw.markdown"
      ],
      "settings": { "foreground": "#9E9E9E" }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold", "foreground": "#FFFFFF" }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic", "foreground": "#FFFFFF" }
    },
    {
      "name": "Markdown Link Text",
      "scope": "string.other.link.title.markdown",
      "settings": { "foreground": "#7CD992" }
    },
    {
      "name": "Markdown Link URL",
      "scope": "markup.underline.link.markdown",
      "settings": { "foreground": "#CD93FF" }
    },
    {
      "name": "Markdown Raw Code",
      "scope": [
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": { "foreground": "#F14C4C" }
    },
    {
      "name": "CSS Tag/Selector Name",
      "scope": "entity.name.tag.css",
      "settings": { "foreground": "#F14C4C", "fontStyle": "bold" }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id.css",
      "settings": { "foreground": "#56D4BC" }
    },
    {
      "name": "CSS Class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": { "foreground": "#7CD992" }
    },
    {
      "name": "CSS Property Name",
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#FFFFFF" }
    },
    {
      "name": "CSS Property Value",
      "scope": [
        "support.constant.property-value.css",
        "support.constant.font-name.css"
      ],
      "settings": { "foreground": "#CD93FF" }
    },
    {
      "name": "CSS Units",
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#CD93FF" }
    }
  ]
}