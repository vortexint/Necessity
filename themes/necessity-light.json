    "terminal.ansiBrightBlue": "#007ACC",
    "terminal.ansiGreen": "#008000",
    "terminal.ansiBrightGreen": "#20A020",
    "terminal.ansiCyan": "#008080",
    "terminal.ansiBrightCyan": "#20A0A0",
