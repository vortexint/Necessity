  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python"
      ],
      "settings": {
        "foreground": "#1E1E1E"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#505050",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.tag",
        "string.value",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#8B0000"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "#006400"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage",
        "storage.type",
        "storage.modifier",
        "keyword.operator.word",
        "keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.unit",
        "variable.language"
      ],
      "settings": {
        "foreground": "#562D80",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#3A3A3A"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.method"
      ],
      "settings": {
        "foreground": "#4B0082",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.enum",
        "entity.other.inherited-class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#004C5C"
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#5A5A5A"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#1E1E1E"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#3A3A3A"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#562D80"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4B0082"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "keyword.control.directive"
      ],
      "settings": {
        "foreground": "#5A5A5A"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#2A2A2A"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#562D80",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#C51A1B",
        "fontStyle": "underline italic"
      }
    }
  ],
  "semanticTokenColors": {
    "enumMember": "#006400",
    "variable.declaration.local": {
      "foreground": "#1E1E1E"
    },
    "variable.defaultLibrary": "#004C5C",
    "property": "#1E1E1E",
    "parameter": {
      "foreground": "#3A3A3A"
    },
    "type": "#004C5C",
    "class": "#004C5C",
    "struct": "#004C5C",
    "interface": "#004C5C",
    "enum": "#004C5C",
    "function": "#4B0082",
    "method": "#4B0082",
    "namespace": "#5A5A5A",
    "macro": "#5A5A5A",
    "keyword": "#562D80",
    "string": "#8B0000",
    "number": "#006400",
    "regexp": "#8B0000",
    "operator": "#3A3A3A",
    "comment": {
      "foreground": "#505050",
      "italic": true
    }
  }
