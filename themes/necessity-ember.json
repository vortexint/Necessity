  "tokenColors": [
    {
      "name": "Default Text",
      "scope": [
        "source", "text", "meta.brace", "punctuation", "meta.tag", "meta.embedded"
      ],
      "settings": { "foreground": "#E0E0E0" }
    },
    {
      "name": "Variables, Object Keys, Constants ",
      "scope": [
        "variable", "variable.other.readwrite", "variable.other.object", "variable.other.constant",
        "support.variable", "entity.name.variable", "meta.object-literal.key"
      ],
      "settings": { "foreground": "#FFFFFF" }
    },
    {
      "name": "Parameters (Optional Italic)",
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#E0E0E0", "fontStyle": "italic" }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor", "entity.name.function.preprocessor",
        "keyword.control.directive", "punctuation.definition.directive"
      ],
      "settings": { "foreground": "#FF8080" }
    },
    {
      "name": "General Keywords, Storage, Modifiers, Operators",
      "scope": [
        "keyword", "keyword.control", "keyword.operator", "storage", "storage.modifier"
      ],
      "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
    },
    {
      "name": "Standard Built-in Types, Namespace/Using",
      "scope": [
        "storage.type", "support.type.primitive", "keyword.other.namespace", "storage.type.namespace"
      ],
      "settings": { "foreground": "#80B4FF" }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#909090", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#B4E099" }
    },
    {
      "name": "Numbers and Language Constants",
      "scope": [
        "constant.numeric", "constant.language", "constant.character",
        "constant.character.escape"
      ],
      "settings": { "foreground": "#F0A0A0" }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function", "support.function", "meta.function-call entity.name.function"
      ],
      "settings": { "foreground": "#DCDCAA" }
    },
    {
      "name": "Classes, User-Defined Types, Structs, Interfaces",
      "scope": [
        "entity.name.type", "entity.name.class", "entity.name.struct", "entity.name.interface",
        "entity.name.enum", "support.type", "support.class"
      ],
      "settings": { "foreground": "#FFA570", "fontStyle": "bold" }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#90EE90", "fontStyle": "italic" }
    },
   
    {
      "name": "Tag Brackets",
      "scope": "punctuation.definition.tag",
      "settings": { "foreground": "#A0A0A0" }
    },
    {
      "name": "Tag Name",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#80B4FF", "fontStyle": "bold" }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#B4E099", "fontStyle": "" }
    },
   
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
    },
    {
      "name": "Markdown Punctuation",
      "scope": [
        "punctuation.definition.heading.markdown", "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown", "punctuation.definition.list.begin.markdown",
        "punctuation.definition.string.begin.markdown", "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown", "punctuation.definition.raw.markdown"
      ],
      "settings": { "foreground": "#A0A0A0" }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold", "foreground": "#FFFFFF" }
    },
    {
      "name": "Markdown Italic",
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic", "foreground": "#E0E0E0" }
    },
    {
      "name": "Markdown Link Text",
      "scope": "string.other.link.title.markdown",
      "settings": { "foreground": "#B4E099" }
    },
    {
      "name": "Markdown Link URL",
      "scope": "markup.underline.link.markdown",
      "settings": { "foreground": "#D8A0E0" }
    },
    {
      "name": "Markdown Raw Code",
      "scope": ["markup.inline.raw.string.markdown", "markup.fenced_code.block.markdown"],
      "settings": { "foreground": "#FFA570" }
    },
   
    {
      "name": "CSS Tag/Selector Name",
      "scope": "entity.name.tag.css",
      "settings": { "foreground": "#FF8080", "fontStyle": "bold" }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id.css",
      "settings": { "foreground": "#87CEFA" }
    },
    {
      "name": "CSS Class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": { "foreground": "#B4E099" }
    },
    {
      "name": "CSS Property Name",
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#E0E0E0" }
    },
    {
      "name": "CSS Property Value & Units",
      "scope": [
        "support.constant.property-value.css", "support.constant.font-name.css", "keyword.other.unit"
      ],
      "settings": { "foreground": "#D8A0E0" }
    }
  ]
}